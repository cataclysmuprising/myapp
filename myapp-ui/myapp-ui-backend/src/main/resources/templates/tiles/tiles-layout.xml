<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
		"http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
	<definition name="baseLayout" template="/WEB-INF/views/template/default/baseLayout.jsp">
		<put-attribute name="appName" value="portal-backend"/>
		<put-attribute name="header" value="/WEB-INF/views/template/default/header.jsp"/>
		<put-attribute name="leftMenu" value="/WEB-INF/views/template/default/main-sidebar.jsp"/>
		<put-attribute name="content" value=""/>
		<put-attribute name="rightMenu" value="/WEB-INF/views/template/default/control-sidebar.jsp"/>
		<put-attribute name="footer" value="/WEB-INF/views/template/default/footer.jsp"/>
		<put-list-attribute name="layoutScripts">
			<add-attribute value="common/pace"/>
			<add-attribute value="common/moment"/>
			<add-attribute value="common/fastclick"/>
			<add-attribute value="common/icheck"/>
			<add-attribute value="common/lobibox-notifications"/>
			<add-attribute value="common/tooltipster.bundle"/>
			<add-attribute value="common/jquery.slimscroll"/>
			<add-attribute value="common/bootstrap-select"/>
			<add-attribute value="common/bootstrap-datetimepicker"/>
			<add-attribute value="theme/AdminLTE/theme"/>
			<add-attribute value="theme/AdminLTE/app"/>
		</put-list-attribute>
		<put-list-attribute name="layoutStyles">
			<add-attribute value="common/pace"/>
			<add-attribute value="common/animate"/>
			<add-attribute value="common/icheck-minimal-red"/>
			<add-attribute value="common/bootstrap-select"/>
			<add-attribute value="common/bootstrap-datetimepicker"/>
			<add-attribute value="common/lobibox"/>
			<add-attribute value="common/tooltipster.bundle"/>
			<add-attribute value="theme/AdminLTE/theme"/>
			<add-attribute value="theme/AdminLTE/skins"/>
			<add-attribute value="theme/AdminLTE/app"/>
		</put-list-attribute>
	</definition>
</tiles-definitions>